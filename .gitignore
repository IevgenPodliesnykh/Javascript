<html>
<head>
<meta charset="utf-8">
</head>
<body>
<script>
var Human = function(name, age) {
this.name = name;
this.age = age;
}

var Nikolay = new Human('Nikolay', 26);
var Borya = new Human('Borya', 32);
var Sasha = new Human('Sasha', 24);
var Sergey = new Human('Sergey', 48);

var people = [];
people.push();

var people = [
    ['Nikolay', 26],
    ['Borya', 32],
    ['Sasha', 24],
    ['Sergey', 48]
];

function sAge(i, ii) { // По возрасту (возрастание)
    if (i[1] > ii[1])
        return 1;
    else if (i[1] < ii[1])
        return -1;
    else
        return 0;
}

document.write(people.sort(sAge));
</script>
</body>
</html>
